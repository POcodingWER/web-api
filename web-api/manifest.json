  "related_applications": [
    {
      "platform": "webapp",
      "url": "https://web-apis-demo.example.com/manifest.json"
    }
  ],
  "prefer_related_applications": false,
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": [
            "image/*",
            "text/*",
            "application/json"
          ]
        }
      ]
    }
  },
  "file_handlers": [
    {
      "action": "/handle-file",
      "accept": {
        "application/json": [
          ".json"
        ],
        "text/plain": [
          ".txt",
          ".md"
        ],
        "image/*": [
          ".png",
          ".jpg",
          ".jpeg",
          ".gif",
          ".webp",
          ".svg"
        ]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+webapis",
      "url": "/handle-protocol?url=%s"
    }
  ],
  "capture_links": "new-client",
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "edge_side_panel": {
    "preferred_width": 400
  },
  "handle_links": "preferred",
